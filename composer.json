{
    "name": "tarlepp/symfony-backend",
    "description": "Simple JSON API which is build on top of Symfony framework",
    "type": "project",
    "keywords": [
        "php",
        "symfony",
        "REST",
        "JSON"
    ],
    "homepage": "https://github.com/tarlepp/symfony-backend",
    "license": "MIT",
    "authors": [
        {
            "name": "Tarmo LeppÃ¤nen",
            "homepage": "https://github.com/tarlepp",
            "role": "developer"
        }
    ],
    "support": {
        "issues": "https://github.com/tarlepp/symfony-backend/issues"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "3.0.4",
        "doctrine/orm": "2.5.4",
        "doctrine/doctrine-bundle": "1.6.2",
        "doctrine/doctrine-cache-bundle": "1.3.0",
        "symfony/swiftmailer-bundle": "2.3.11",
        "symfony/monolog-bundle": "2.10.0",
        "sensio/distribution-bundle": "5.0.5",
        "sensio/framework-extra-bundle": "3.0.13",
        "incenteev/composer-parameter-handler": "2.1.2",
        "knplabs/doctrine-behaviors": "1.3.2",
        "friendsofsymfony/rest-bundle": "2.0.0-BETA2",
        "jms/serializer-bundle": "1.1.0",
        "lexik/jwt-authentication-bundle": "1.5.1"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tarlepp/symfony-backend"
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "platform": {
            "php": "5.5.9"
        }
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
